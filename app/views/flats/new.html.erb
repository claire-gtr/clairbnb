<div class="container">
  <div class="row">
    <div class="col">
      <%= simple_form_for @flat do |f| %>
        <%= f.input :title %>
        <%= f.input :description %>
        <%= f.input :address %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
